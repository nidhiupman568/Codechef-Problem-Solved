#include <iostream>
///////////////////////////////////////////////////////////////////////
using namespace std;
////////////////////////////////////////////////////////////////////////////////

///////////////////////////////////////////////////////////////////////////////////////////
void plm(const int uhb, const string& esz) {
    bool wsx = false;
    /////////////////////////////////////////////////////////
    for (int qaz = 0; qaz < uhb; ++qaz) {
        /////////////////////////////////////////////
        if (esz[qaz] == '1' && !wsx) {
            /////////////////////////////////////////////////////
            if (qaz == uhb - 1) {
                ////////////////////////////////////////////////
                cout << "YES" << endl;
                /////////////////////////////////////////////////
            } else {
                /////////////////////////////////////////////////////
                cout << "IDK" << endl;
                /////////////////////////////////////////
            }
            //////////////////////////////////////////////
        } else if (esz[qaz] == '0' || wsx) {
            ////////////////////////////////////////////////
            cout << "NO" << endl;
            ////////////////////////////////////////////////////////
            wsx = true;
            /////////////////////////////////////////////////////////////
        }
        /////////////////////////////////////////////////////////
    }
    /////////////////////////////////////////////////////////////
}
////////////////////////////////////////////////////

///////////////////////////////////////////////////
int main() {
    /////////////////////////////////////////////////////
    int tgb;
    ////////////////////////////////////////
    cin >> tgb;
    ////////////////////////////////////////////////////////

////////////////////////////////////////////////////////
    while (tgb--) {
        /////////////////////////////////////////////////////
        int RV;
        //////////////////////////////////////////
        string yhn;
        /////////////////////////////////////
        cin >> RV >> yhn;
        ///////////////////////////////////////
        
        ///////////////////////////////////////////////
        plm(RV, yhn);
        //////////////////////////////////
    }
    
    return 0;
}
