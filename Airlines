#include <iostream>
//////////////////////////////////////

/////////////////////////////////////
struct AircraftCounter {
    /////////////////////////////////////////////////////////////////////////
    int calculateAircraftCount(int initialPos, int totalDistance) {
        /////////////////////////////////////////////////
        int convertedPos = initialPos * 100;
        ////////////////////////////////////////////////////////////////
        int remainingDistance = totalDistance - convertedPos;
        //////////////////////////////////////////
        int count_aircraft = 0;
        /////////////////////////////////////////////

/////////////////////////////////////////////////////
        while (remainingDistance > 0) {
            ////////////////////////////////////////////
            count_aircraft += 1;
            /////////////////////////////////////////////
            remainingDistance -= 100;
            /////////////////////////////////////////
        }
        ///////////////////////////////////////////////
        
        /////////////////////////////////////////////////
        return count_aircraft;
        ///////////////////////////////////////////////////
    }
    /////////////////////////////////////////////////////////
};
////////////////////////////////////////////////////////////

int main() {
    ///////////////////////////////////////////////////////////
    int testCases;
    //////////////////////////////////////////////////////////
    std::cin >> testCases;
    ////////////////////////////////////////////////////////

//////////////////////////////////////////////////////////////////
    AircraftCounter aircraftCounter;
    //////////////////////////////////////////////////////////////

///////////////////////////////////////////////////////////
    for (int i = 0; i < testCases; ++i) {
        /////////////////////////////////////////////////////////////
        int initialPos, totalDistance;
        ////////////////////////////////////////////////////////////
        std::cin >> initialPos >> totalDistance;
        ///////////////////////////////////////////

        int aircraftCount = aircraftCounter.calculateAircraftCount(initialPos, totalDistance);
        ///////////////////////////////////////////////////////////////////
        std::cout << aircraftCount << std::endl;
        /////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    }
    //////////////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    return 0;
}
