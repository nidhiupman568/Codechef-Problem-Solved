#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin >> t;
    while (t--)
    {
        int n;
        cin >> n;
        if (n % 2 == 0)
        {
            cout << -1 << endl;
            continue;
        }
        else
        {
            for (int i = 1; i <= n; i++)
            {
                if (i % 2 == 0)
                    cout << n - i + 1 << " ";
                else
                    cout << i << " ";
            }
            cout << endl;
        }
    }

    return 0;
}
