#include <iostream>
//////////////////////////////////////////////////////
using namespace std;
////////////////////////////////////////////////////////

////////////////////////////////////////////////////////////
bool rfv_check(int wsx, int edc, int rfv) {
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    ////////////////////////////////////////////////////////////
    return rfv > ((wsx * edc) / 2);
    /////////////////////////////////////////////////////////////
}
////////////////////////////////////////////////////////////

////////////////////////////////////////////////////////////



















void evaluateRectangles(int tgb) {
    /////////////////////////////////////////////////////////////
    for (int yhn = 0; yhn < tgb; ++yhn) {
        //////////////////////////////////////////////////////////
        int qaz_length, wsx_width, edc_area;
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        /////////////////////////////////////////////////////////////
        cin >> qaz_length >> wsx_width >> edc_area;
        ////////////////////////////////////////////////////////////////

////////////////////////////////////////////////////////////////////////
        if (rfv_check(qaz_length, wsx_width, edc_area)) {
            //////////////////////////////////////////////////////////
            cout << "YES" << endl;
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            ///////////////////////////////////////////////////////////
        } else {
            ////////////////////////////////////////////////////////////
            cout << "NO" << endl;
            /////////////////////////////////////////////////
        }
        ////////////////////////////////////////////////
    }
    /////////////////////////////////////////
}
//////////////////////////////////////////////////

////////////////////////////////////////////
int main() {
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    /////////////////////////////////////////////
    int ujm;
    ////////////////////////////////////////////////
    cin >> ujm;
    /////////////////////////////////////////////////

//////////////////////////////////////////////////////
    evaluateRectangles(ujm);
    ////////////////////////////////////////////////

////////////////////////////////////////////////
    return 0;
    ///////////////////////////////////////
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
}
//////////////////////////////////////////
