#include <iostream>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--)
	{
	    int n,k;
	    cin >> n >> k;
	    if(k==n-1 )
	    cout << "-1";
	    else if(k==n)
	    {
	        for(int i=0;i<n;i++)
	        cout << i+1 << " ";
	    }
	    else
	    {
	        for(int i=0;i<k;i++)
	        {
	            cout << i+1 << " ";
	        }
	        cout << n << " ";
	        for(int i=k;i<n-1;i++)
	        {
	            cout << i+1 << " ";
	        }
	    }
	    cout << "\n";
	}
	
	return 0;
}
