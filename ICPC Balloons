#include <iostream>
using namespace std;

int main() {
    int t,n;
    cin>>t;
    while(t--)
    {
       cin>>n;
       int a[n];
       for(int i=0;i<n;i++)
       {
           cin>>a[i];
       }
       
       int last=0;
       for(int i=0;i<n;i++)
       {
           if(a[i]<8)
           last=i+1;
           
       }
       
       cout<<last<<endl;
    }
	return 0;
}
