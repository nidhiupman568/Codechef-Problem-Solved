#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int n;
	    scanf("%d",&n);
	    char s[n+1];
	    scanf("%s",&s);
	    for(int i=0;i<n;i++){
	        s[i]=(s[i]=='0' ?'1' : '0');
	    }
	    printf("%s\n",s);
	}
	return 0;

}
