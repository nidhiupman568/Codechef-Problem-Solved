
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    if(n%2!=0){
	        if(n==1){
	            cout<<"Yes"<<endl;
	        }
	        else {
	            cout<<"no"<<endl;
	        }
	    }
	    else {
	        cout<<"Yes"<<endl;
	    }
	}
	return 0;
}
