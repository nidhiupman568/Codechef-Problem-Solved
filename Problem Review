#include <bits/stdc++.h>
using namespace std;

bool isGood(int s[], int n)
{
    for(int i=0; i<n; i++){
    if(s[i] <=4)
    {
        return false;
        break;
    }
    }
    return true;
    
}

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int s[n];
       
        for(int i=0; i<n; i++)
        {
            cin>>s[i];
        }
            if(isGood(s,n))
            {
                cout<<"YES"<<endl;
            }
            else
            {
                cout<<"NO"<<endl;
            }
    }
}

