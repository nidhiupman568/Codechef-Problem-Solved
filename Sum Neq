#include <iostream>
//////////////////////////////////////
#include <vector>
////////////////////////////////////
#include <unordered_set>
///////////////////////////////////
using namespace std;
///////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////
void checkAndPrintResult(const vector<int>& qaz) {
    //////////////////////////////////
    int wsx = qaz.size();
    //////////////////////////////////////////////////////
    unordered_set<int> edc(qaz.begin(), qaz.end());
    ////////////////////////////////////////////

/////////////////////////////////
    if (wsx == 4) {
        ////////////////////////////////////////////////
        if (qaz[0] + qaz[1] == qaz[2] + qaz[3]) {
            ///////////////////////////////////
            cout << "NO" << endl;
            /////////////////////////////////
        } else {
            ////////////////////////////
            cout << "YES" << endl;
            ///////////////////////////////
        }
        ////////////////////////////////
        return;
        ///////////////////////////////
    }
    ///////////////////////////////////

////////////////////////////////////
    if (edc.size() == 1) {
        ////////////////////////////////
        cout << "NO" << endl;
        /////////////////////////////////
    } else {
        //////////////////////////////////
        cout << "YES" << endl;
        ////////////////////////////
    }
    ////////////////////////////
}
///////////////////////////////////////

////////////////////////////////////////
void solveTestCase() {
    /////////////////////////////
    int rfv;
    /////////////////////////////////
    cin >> rfv;
    ////////////////////////////////////

///////////////////////////////////////
    vector<int> tgb(rfv);
    ///////////////////////////////////////////////
    for (int yhn = 0; yhn < rfv; ++yhn) {
        ///////////////////////////////////
        cin >> tgb[yhn];
        ////////////////////////////////////////
    }
    ////////////////////////////////////////////

/////////////////////////////////////////////
    checkAndPrintResult(tgb);
    //////////////////////////////////////
}
/////////////////////////////////////////////

////////////////////////////////////////////
int main() {
    //////////////////////////////////////
    int ujm;
    /////////////////////////////////////////
    cin >> ujm;
    ////////////////////////////////////

/////////////////////////////////////////////////////
    for (int plm = 0; plm < ujm; ++plm) {
        ////////////////////////////////
        solveTestCase();
        //////////////////////////
    }
    //////////////////////////

////////////////////////
    return 0;
    //////////////////////
}
///////////////////////
