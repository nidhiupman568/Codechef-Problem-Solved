#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
int T;
    cin >> T;

    while (T--) {
        int N, K;
        cin >> N >> K;

        int min_slices = 10 * N;
        int max_slices = 12 * N;

        if (K >= min_slices && K <= max_slices) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
