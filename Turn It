#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        int u, a, s;
        double v, sqrtvel;
        cin >> u >> v >> a >> s;
        int vel = (u * u) - (2 * a * s);
        sqrtvel = sqrt(vel);

        if (sqrtvel > v)
            cout << "NO" << endl;
        else
            cout << "YES" << endl;
    }

    return 0;
}
