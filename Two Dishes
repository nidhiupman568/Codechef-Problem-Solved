#include <iostream>
using namespace std;

int main() {
    int num_tests;
    cin >> num_tests;
    
    while (num_tests--) {
        int n, apples, bananas, oranges;
        cin >> n >> apples >> bananas >> oranges;
        
        int fruit_fish = apples + oranges;
        if (n - fruit_fish <= 0 && n - bananas <= 0) {
            cout << "YES" << endl;
        }
        else {
            cout << "NO" << endl;
        }
    }
    return 0;
}
