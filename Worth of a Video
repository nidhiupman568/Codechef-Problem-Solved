#include <iostream>
///////////////////////////////////////////////////

//////////////////////////////////////////////////
int calculateSoldiers(int xz) {
    //////////////////////////////////////////
    return xz * 24 * 1000;
    ////////////////////////////////
}
//////////////////////////////////////////////

/////////////////////////////////////////
int main() {
    //////////////////////////////////////////////
    int uniqueVar;
    ////////////////////////////////////////////////////
    std::cin >> uniqueVar;
    ///////////////////////////////////////////////

///////////////////////////////////////////////
    while (uniqueVar--) {
        /////////////////////////////////////////
        int uniqueInput;
        //////////////////////////////////////////////////
        std::cin >> uniqueInput;
        //////////////////////////////////////////////////////////
        
        /////////////////////////////////////////////////////////////
        int uniqueResult = calculateSoldiers(uniqueInput);
        ////////////////////////////////////////////////////////////
        std::cout << uniqueResult << std::endl;
        ///////////////////////////////////////////////////////
    }
    ///////////////////////////////////////////////

///////////////////////////////////////
    return 0;
    /////////////////////////////////////////
}
//////////////////////////////////////////
