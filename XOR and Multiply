
#include <iostream>
#include <bits/stdc++.h>
using namespace std;

#define int long long

int32_t main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    auto solve = [&]()->void{
        	int x ;
        	cin>>x ;
        	int a = x ;
        	int k = log2(a);
			int b = (x<<(k+1)) + x ;
			cout<<a<<" "<<b<<endl;
    };
    

int test = 1 ;
cin>>test;
while(test--)
solve();
return 0;
}   
